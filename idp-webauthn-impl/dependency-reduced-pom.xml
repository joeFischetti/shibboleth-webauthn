<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>edu.duke.oit.idms</groupId>
  <artifactId>idp-authn-webauthn-impl</artifactId>
  <name>Shibboleth IdP v3 webauthn plugin</name>
  <version>0.1.0</version>
  <description>WebAuthn authentication module for Shibboleth IdP v3</description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>javax.servlet-api</artifactId>
      <version>4.0.1</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <properties>
    <mobile.auth.version>1.0</mobile.auth.version>
    <java-support.version>7.5.0</java-support.version>
    <shib.idp.version>3.4.5</shib.idp.version>
    <com.warrenstrange.version>0.5.0</com.warrenstrange.version>
    <gson.version>2.3.1</gson.version>
    <maven.compiler.target>1.7</maven.compiler.target>
    <maven.compiler.version>3.2</maven.compiler.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.compiler.source>1.7</maven.compiler.source>
    <org.springframework.ldap.version>2.0.4.RELEASE</org.springframework.ldap.version>
  </properties>
</project>

